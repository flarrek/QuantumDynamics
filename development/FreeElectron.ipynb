    "using QuantumDynamics"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "X = 1000.\n",
    "system = System( FreeHamiltonian() , Electron() ; dimensions=(-X,X) )\n",
    "\n",
    "state = ContinuousState( Position , x->(x+X)*(x-X) ; system=system )\n",
    "\n",
    "evolve!( state ; duration=1. , steps=100 )\n",
    "\n",
    "animate( state )"
   ]
  }
